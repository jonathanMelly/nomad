{
	"Version": "0.78",
	"ApplicationName": "putty",
	"Shortcut": "PUTTY.bat",
	"ShortcutIcon": "PUTTY.EXE,0",
	"DownloadExtension": ".zip",
	"DownloadUrl": "https://github.com/NoMoreFood/putty-cac/raw/{{VERSION}}/binaries/puttycac-64bit-{{VERSION}}.zip",
	"RemoveRootFolder": false,
	"VersionCheck":
	{
		"Url":"https://api.github.com/repos/NoMoreFood/putty-cac/releases/latest",
		"RegEx":"\"tag_name\": \"{{VERSION}}"
	},
	"ExtractRegExList":
	[
		"(.*)"
	],
	"CreateFiles":
	{
		"VERSION-{{VERSION}}.txt":"{{VERSION}}",
		"putty.reg":"Windows Registry Editor Version 5.00\r\n\r\n[HKEY_CURRENT_USER\\Software\\SimonTatham\\PuTTY]\r\n",
		"PUTTY.bat":"@ECHO OFF\r\ntasklist | findstr /I putty || cd %papps%putty && reg import putty.reg && reg import puttyrnd.reg && start /w putty.exe && reg export HKEY_CURRENT_USER\\Software\\SimonTatham\\PuTTY new.reg && copy new.reg putty.reg && del new.reg",
		"PUTTYRND.reg":"Windows Registry Editor Version 5.00\r\n\r\n[HKEY_CURRENT_USER\\Software\\SimonTatham\\PuTTY]\r\n\"RandSeedFile\"=\"%papps%\\\\putty\\\\putty.rnd\""
	},
	"RestoreFiles":
	[
		"PUTTY.RND","putty.reg","wsl-ssh-pageant-amd64-gui.exe"
	]
}