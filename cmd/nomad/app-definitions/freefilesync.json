{
	"Version": "11.28",
	"ApplicationName": "freefilesync",
	"Shortcut": "freefilesync.exe",
	"DownloadExtension": ".zip",
	"DownloadUrl": "manual,go to about in app to get link : https://freefilesync.org/thank-you.php?tx=zzz",
	"RemoveRootFolder": false,
	"VersionCheck":
	{
		"Url":"https://freefilesync.org/download.php",
		"RegEx":"content=\"Download FreeFileSync {{VERSION}}"
	},
	"ExtractRegExList":
	[
		"(.*)"
	],
	"CreateFiles":
	{
		"VERSION-{{VERSION}}.txt":"{{VERSION}}"
	},
	"RestoreFiles":
	[
		"GlobalSettings.xml",
		"Logs/",
		"LastRun.ffs_gui",
		"config/"
	]
}