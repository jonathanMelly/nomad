{
	"Version": "8.4.7",
	"ApplicationName": "notepad++",
	"Shortcut": "notepad++.exe",
	"DownloadUrl": "https://github.com/notepad-plus-plus/notepad-plus-plus/releases/download/v{{VERSION}}/npp.{{VERSION}}.portable.x64.zip",
	"VersionCheck":
	{
		"Url":"https://github.com/notepad-plus-plus/notepad-plus-plus/releases/latest",
		"RegEx":"<title>Release Notepad\\+\\+ release ({{VERSION}})"
	},
	"RemoveRootFolder": false,
	"ExtractRegExList":
	[
		"(.*)"
	],
	"CreateFiles":
	{
		"VERSION-{{VERSION}}.txt":"{{VERSION}}"
	},
	"RestoreFiles":
	[
		"userDefineLangs/userDefineLang_AHK.xml"
	]
}