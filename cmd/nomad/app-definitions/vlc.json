{
	"Version": "3.0.18",
	"ApplicationName": "vlc",
	"Shortcut": "vlc.exe",
	"DownloadUrl": "https://download.videolan.org/pub/videolan/vlc/{{VERSION}}/win64/vlc-{{VERSION}}-win64.zip",
	"VersionCheck":
	{
		"Url":"https://www.videolan.org/vlc/",
		"RegEx":"Version <span id='downloadVersion'>\\s*{{VERSION}}"
	},
	"CreateFiles":
	{
		"VERSION-{{VERSION}}.txt":"{{VERSION}}"
	},
	"RestoreFiles":
	[
		"portable/"
	]
}