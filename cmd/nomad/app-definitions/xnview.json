{
	"Version": "1.4.2",
	"ApplicationName": "xnview-{{VERSION}}",
	"Symlink": "xnview",
	"Shortcut": "xnview.exe",
	"DownloadExtension": ".zip",
	"DownloadUrl": "https://download.xnview.com/XnViewMP-win-x64.zip",
	"RemoveRootFolder": true,
	"VersionCheck":
	{
		"Url":"https://www.xnview.com/en/xnviewmp/#changelog",
		"RegEx":"<span class=\"h5 fw-bold\">{{VERSION}} <small"
	},
	"ExtractRegExList":
	[
		"(.*)"
	],
	"CreateFiles":
	{
		"VERSION-{{VERSION}}.txt":"{{VERSION}}"
	}
}