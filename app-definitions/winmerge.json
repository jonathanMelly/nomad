{
	"Version": "2.16.24",
	"ApplicationName": "winmerge-{{VERSION}}",
	"Symlink": "winmerge",
	"Shortcut": "WinMergeU.exe",
	"DownloadExtension": ".zip",
	"DownloadUrl": "https://github.com/WinMerge/winmerge/releases/download/v{{VERSION}}/winmerge-{{VERSION}}-exe.zip",
	"RemoveRootFolder": true,
	"VersionCheck":
	{
		"Url":"https://api.github.com/repos/WinMerge/winmerge/releases/latest",
		"RegEx":"\"tag_name\": \"v{{VERSION}}",
		"UseLatestVersion": true
	},
	"ExtractRegExList":
	[
		"(.*)"
	],
	"CreateFiles":
	{
		"VERSION-{{VERSION}}.txt":"{{VERSION}}"
	}
}