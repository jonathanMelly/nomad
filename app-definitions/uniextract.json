{
	"Version": "2.0.0-rc.3",
	"ApplicationName": "uniextract-{{VERSION}}",
	"Symlink": "uniextract",
	"DownloadExtension": ".zip",
	"DownloadUrl": "https://github.com/Bioruebe/UniExtract2/releases/download/v{{VERSION}}/UniExtractRC3.zip",
	"VersionCheck":
	{
		"Url":"https://api.github.com/repos/Bioruebe/UniExtract2/releases",
		"RegEx":"\"tag_name\": \"v{{VERSION}}"
	},
	"RemoveRootFolder": true,
	"ExtractRegExList":
	[
		"(.*)"
	],
	"CreateFiles":
	{
		"VERSION-{{VERSION}}.txt":"{{VERSION}}"
	},
	"RestoreFiles":
	[
		"UniExtract.ini","passwords.txt","log/"
	]
}