{
	"Version": "1.0.2",
	"ApplicationName": "regextestbench-{{VERSION}}",
	"Symlink": "regextestbench",
	"Shortcut": "RegexTestBench.Wpf.exe",
	"DownloadExtension": ".zip",
	"DownloadUrl": "https://github.com/CommonLoon102/RegexTestBench/releases/download/v{{VERSION}}/RegexTestBench-win-x86.zip",
	"RemoveRootFolder": true,
	"VersionCheck":
	{
		"Url":"https://api.github.com/repos/CommonLoon102/RegexTestBench/releases/latest",
		"RegEx":"\"tag_name\": \"v{{VERSION}}"
	},
	"ExtractRegExList":
	[
		"(.*)"
	],
	"CreateFiles":
	{
		"VERSION-{{VERSION}}.txt":"{{VERSION}}"
	}
}