{
	"Version": "1.2.10.6",
	"ApplicationName": "greenshot-{{VERSION}}",
	"Symlink": "greenshot",
	"DownloadExtension": ".zip",
	"DownloadUrl": "https://github.com/greenshot/greenshot/releases/download/v{{VERSION}}/ShareX-{{VERSION}}-portable.zip",
	"RemoveRootFolder": false,
	"VersionCheck":
	{
		"Url":"https://api.github.com/repos/ShareX/ShareX/releases/latest",
		"RegEx":"\"tag_name\": \"v{{VERSION}}"
	},
	"ExtractRegExList":
	[
		"(.*)"
	],
	"CreateFiles":
	{
		"VERSION-{{VERSION}}.txt":"{{VERSION}}"
	},
	"RestoreFiles":
	[
		"backups/"
	]
}