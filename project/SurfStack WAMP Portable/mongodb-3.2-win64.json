{
	"ApplicationName": "MongoDB{{VERSION}}",
	"DownloadExtension": ".msi",
	"DownloadUrl": "https://fastdl.mongodb.org/win32/mongodb-win32-x86_64-{{VERSION}}-signed.msi",
	"RemoveRootFolder": true,
	"RootFolderName": "MongoDB/Server/3.2",
	"ExtractRegExList":
	[
		"GNU-AGPL-3.0",
		"PL-2",
		"README",
		"THIRD-PARTY-NOTICES",
		"bin/mongo.exe",
		"bin/mongod.exe",
		"bin/mongoexport.exe",
		"bin/mongoimport.exe",
		"bin/mongos.exe"
	],
	"CreateFolders":
	[
		"bin/data"
	]
}